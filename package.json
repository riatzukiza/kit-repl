{
    "name": "kit-repl",
    "version": "1.4.1",
    "description": "A networked repl for an extension of sibilant I call kit.",
    "main": "index.js",
    "dependencies": {
        "sibilant": "*",
        "tree-kit": "git@github.com:riatzukiza/tree-kit.git",
        "kit": "git@github.com:riatzukiza/kit.git",
        "kit-actor": "git@github.com:riatzukiza/kit-actor.git",
        "kit-events": "git@github.com:riatzukiza/kit-events.git",
        "kit-interface": "git@github.com:riatzukiza/kit-interface.git"
    },
    "devDependencies": {},
    "scripts": {
        "local": "sibilant -x cli.sibilant",
        "compile": "sibilant ./src/*.sibilant -o ./js",
        "publish": "npm run compile;git add .;git commit -m 'new version published';git push oversoul master;"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:riatzukiza/kit-repl.git"
    },
    "keywords": [
        "cli",
        "sibilant",
        "lisp"
    ],
    "author": "Aaron Beavers",
    "license": "ISC",
    "homepage": "https://bitbucket.org/aaron_beavers/kit-repl#readme"
}
